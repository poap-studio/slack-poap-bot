{
  "version": 2,
  "functions": {
    "api/index.js": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/slack/events",
      "dest": "/api/index.js"
    },
    {
      "src": "/health",
      "dest": "/api/index.js"
    },
    {
      "src": "/poap-rules",
      "dest": "/api/index.js"
    },
    {
      "src": "/poap-rules/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/slack-channels",
      "dest": "/api/index.js"
    },
    {
      "src": "/admin",
      "dest": "/admin.html"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}