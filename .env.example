SLACK_ACCESS_TOKEN=your_slack_access_token_here
SLACK_REFRESH_TOKEN=your_slack_refresh_token_here

# Port for the Express server
PORT=3000

# Email configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_email_password

# POAP API configuration
POAP_API_KEY=your_poap_api_key
POAP_API_URL=https://api.poap.xyz

# Database URL (for production deployment)
DATABASE_URL=postgresql://username:password@localhost:5432/slack_poap_app

# Environment
NODE_ENV=development